{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "USMLE Knowledge Engine",
  "version": "1.0.0",
  "identifier": "com.jiahaoji.usmle-knowledge-engine", 
  "build": {
    "frontendDist": "../dist", 
    "devUrl": "http://localhost:5173", 
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "USMLE Knowledge Engine",
        "width": 1280,
        "height": 720,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "copyright": "Copyright Â© 2025 Jiahao Ji" 
  },
  "plugins": {

    "dialog": null,
    "fs": null
  }
}